{
  "name": "next-dapp-starter",
  "version": "0.1.0",
  "private": true,
  "author": "silashen",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:turbo": "npx turbo build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:turbo": "npx turbo lint",
    "type-check": "tsc --noEmit",
    "type-check:turbo": "npx turbo type-check",
    "test:e2e": "playwright test",
    "format": "prettier --write src",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@chakra-ui/styled-system": "^2.8.0",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@ethersproject/networks": "^5.7.1",
    "@ethersproject/web": "^5.7.1",
    "@rainbow-me/rainbowkit": "^0.12.6",
    "ethers": "^5",
    "framer-motion": "^10.10.0",
    "next": "^13.2.3",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "use-debounce": "^9.0.3",
    "usehooks-ts": "^2.9.1",
    "wagmi": "^0.12.9"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@playwright/test": "^1.31.1",
    "@types/react": "^18.0.31",
    "commitizen": "^4.3.0",
    "commitlint": "^17.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-config-next": "^13.2.3",
    "eslint-config-sznm": "^1.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "next-sitemap": "^4.0.6",
    "prettier": "^2.8.4",
    "standard-version": "^9.5.0",
    "turbo": "^1.8.8",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.x.x",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.1.0"
